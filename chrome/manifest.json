{
  "name": "Meleak",
  "description": "JavaScript Memory Leak Detector for FrontEnd Developer",
  "icons": { 
    "16": "icons/icon.png",
    "48": "icons/icon.png",
   "128": "icons/icon.png" 
  },
  "version": "1.0",
  "permissions": [
    "activeTab",
    "debugger",
    "notifications",
    "storage",
    "webNavigation"
  ],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/index.js"]
    }
  ],
  "web_accessible_resources": [
    "scripts/inject.js"
  ],
  "background": {
    "scripts": [
      "background/actions.js",
      "background/url.js",
      "background/debugger.js",
      "background/reducer.js",
      "background/index.js"
    ]
  },
  "browser_action": {
    "default_icon": "icons/icon.png",
    "default_title": "Meleak",
    "default_popup": "popup/index.html"
  },
  "manifest_version": 2,
  "author": "Mojila"
}